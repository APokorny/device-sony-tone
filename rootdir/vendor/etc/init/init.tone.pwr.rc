on init

    # Disable UFS powersaving
    write /sys/module/lpm_levels/parameters/sleep_disabled Y

on late-fs

    # boot time fs tune
    write /sys/block/dm-0/queue/read_ahead_kb 2048
    write /sys/block/dm-1/queue/read_ahead_kb 2048
    write /sys/block/loop0/queue/read_ahead_kb 2048
    write /sys/block/loop1/queue/read_ahead_kb 2048
    write /sys/block/loop2/queue/read_ahead_kb 2048
    write /sys/block/loop3/queue/read_ahead_kb 2048
    write /sys/block/loop4/queue/read_ahead_kb 2048
    write /sys/block/loop5/queue/read_ahead_kb 2048
    write /sys/block/loop6/queue/read_ahead_kb 2048
    write /sys/block/loop7/queue/read_ahead_kb 2048
    write /sys/block/loop64/queue/read_ahead_kb 2048
    write /sys/block/ram0/queue/read_ahead_kb 2048
    write /sys/block/ram1/queue/read_ahead_kb 2048
    write /sys/block/ram2/queue/read_ahead_kb 2048
    write /sys/block/ram3/queue/read_ahead_kb 2048
    write /sys/block/ram4/queue/read_ahead_kb 2048
    write /sys/block/ram5/queue/read_ahead_kb 2048
    write /sys/block/ram6/queue/read_ahead_kb 2048
    write /sys/block/ram7/queue/read_ahead_kb 2048
    write /sys/block/ram8/queue/read_ahead_kb 2048
    write /sys/block/ram9/queue/read_ahead_kb 2048
    write /sys/block/ram10/queue/read_ahead_kb 2048
    write /sys/block/ram11/queue/read_ahead_kb 2048
    write /sys/block/ram12/queue/read_ahead_kb 2048
    write /sys/block/ram13/queue/read_ahead_kb 2048
    write /sys/block/ram14/queue/read_ahead_kb 2048
    write /sys/block/ram15/queue/read_ahead_kb 2048
    write /sys/block/zram0/queue/read_ahead_kb 2048

on charger

    # Bring CPUs online
    write /sys/devices/system/cpu/cpu1/online 1
    write /sys/devices/system/cpu/cpu2/online 1
    write /sys/devices/system/cpu/cpu3/online 1

    # Configure governor settings for little cluster
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor "powersave"
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 307200

    # Configure governor settings for big cluster
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_governor "powersave"
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_min_freq 307200

    # Bring CPUs offline
    write /sys/devices/system/cpu/cpu1/online 0
    write /sys/devices/system/cpu/cpu2/online 0
    write /sys/devices/system/cpu/cpu3/online 0

    # Enable UFS powersaving
    write /sys/module/lpm_levels/parameters/sleep_disabled N

on property:sys.boot_completed=1

    # End boot time fs tune
    write /sys/block/dm-0/queue/read_ahead_kb 128
    write /sys/block/dm-1/queue/read_ahead_kb 128
    write /sys/block/loop1/queue/read_ahead_kb 128
    write /sys/block/loop2/queue/read_ahead_kb 128
    write /sys/block/loop4/queue/read_ahead_kb 128
    write /sys/block/loop5/queue/read_ahead_kb 128
    write /sys/block/loop7/queue/read_ahead_kb 128
    write /sys/block/loop64/queue/read_ahead_kb 128
    write /sys/block/ram0/queue/read_ahead_kb 128
    write /sys/block/ram1/queue/read_ahead_kb 128
    write /sys/block/ram2/queue/read_ahead_kb 128
    write /sys/block/ram3/queue/read_ahead_kb 128
    write /sys/block/ram4/queue/read_ahead_kb 128
    write /sys/block/ram5/queue/read_ahead_kb 128
    write /sys/block/ram6/queue/read_ahead_kb 128
    write /sys/block/ram7/queue/read_ahead_kb 128
    write /sys/block/ram8/queue/read_ahead_kb 128
    write /sys/block/ram9/queue/read_ahead_kb 128
    write /sys/block/ram10/queue/read_ahead_kb 128
    write /sys/block/ram11/queue/read_ahead_kb 128
    write /sys/block/ram12/queue/read_ahead_kb 128
    write /sys/block/ram13/queue/read_ahead_kb 128
    write /sys/block/ram14/queue/read_ahead_kb 128
    write /sys/block/ram15/queue/read_ahead_kb 128
    write /sys/block/zram0/queue/read_ahead_kb 128

    # enable governor for power cluster
    write /sys/devices/system/cpu/cpu0/online 1
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor "interactive"
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/use_sched_load 0
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/use_migration_notif 1
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/ignore_hispeed_on_notif 0
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/above_hispeed_delay "10000 844800:50000 1228800:40000 1324800:45000 1401600:30000 1478400:20000"
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/go_hispeed_load 99
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/timer_rate 40000
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/hispeed_freq 652800
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/io_is_busy 0
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/align_windows 0
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/target_loads "70 1228800:60 1324800:70 1401600:80 1478400:95"
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/min_sample_time 90000
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/enable_prediction 0
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/fast_ramp_down 1
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 307200
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1593600
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/boost 0
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/max_freq_hysteresis 0
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/timer_slack 20000
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/boostpulse_duration 0

    # enable governor for perf cluster
    write /sys/devices/system/cpu/cpu2/online 1
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_governor "interactive"
    write /sys/devices/system/cpu/cpu2/cpufreq/interactive/use_sched_load 0
    write /sys/devices/system/cpu/cpu2/cpufreq/interactive/use_migration_notif 1
    write /sys/devices/system/cpu/cpu2/cpufreq/interactive/ignore_hispeed_on_notif 0
    write /sys/devices/system/cpu/cpu2/cpufreq/interactive/above_hispeed_delay "10000 883200:50000 1248000:70000 1324800:20000 1478400:15000 1632000:6000 1785600:5000 1996800:3000 2073600:3000 2150400:4000"
    write /sys/devices/system/cpu/cpu2/cpufreq/interactive/go_hispeed_load 99
    write /sys/devices/system/cpu/cpu2/cpufreq/interactive/align_windows 0
    write /sys/devices/system/cpu/cpu2/cpufreq/interactive/timer_rate 10000
    write /sys/devices/system/cpu/cpu2/cpufreq/interactive/hispeed_freq 1324800
    write /sys/devices/system/cpu/cpu2/cpufreq/interactive/io_is_busy 0
    write /sys/devices/system/cpu/cpu2/cpufreq/interactive/target_loads "30 883200:35 1248000:40 1324800:50 1478400:60 1555200:80 1996800:95 2073600:97"
    write /sys/devices/system/cpu/cpu2/cpufreq/interactive/min_sample_time 60000
    write /sys/devices/system/cpu/cpu2/cpufreq/interactive/enable_prediction 0
    write /sys/devices/system/cpu/cpu2/cpufreq/interactive/fast_ramp_down 1
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_min_freq 307200
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_max_freq 2150400
    write /sys/devices/system/cpu/cpu2/cpufreq/interactive/timer_slack 20000
    write /sys/devices/system/cpu/cpu2/cpufreq/interactive/boost 0
    write /sys/devices/system/cpu/cpu2/cpufreq/interactive/max_freq_hysteresis 0
    write /sys/devices/system/cpu/cpu2/cpufreq/interactive/boostpulse_duration 0

    # Bring up all cores online
    write /sys/devices/system/cpu/cpu1/online 1
    write /sys/devices/system/cpu/cpu2/online 1
    write /sys/devices/system/cpu/cpu3/online 1

    # set cpu_boost parameters
    write /sys/module/cpu_boost/parameters/input_boost_freq "0:0 1:0 2:0 3:0"
    write /sys/module/cpu_boost/parameters/input_boost_ms "0"

    write /proc/sys/kernel/sched_spill_nr_run 2
    write /proc/sys/kernel/sched_freq_inc_notify 400000
    write /proc/sys/kernel/sched_freq_dec_notify 400000

	  # set cpuidle parameters
	  write /sys/module/cpuidle/parameters/off 0

    # Setting b.L scheduler parameters
    write /proc/sys/kernel/sched_boost 0
	  write /proc/sys/kernel/sched_cfs_boost 80
    write /proc/sys/kernel/sched_upmigrate 45
    write /proc/sys/kernel/sched_downmigrate 45
    write /proc/sys/kernel/sched_group_upmigrate 100
    write /proc/sys/kernel/sched_group_downmigrate 95

    # Enable accounting on CPUs hwmon and bus speed decision algos
    # This allows to scale bus frequencies based on bandwidth calc
    write /sys/class/devfreq/soc:qcom,cpubw/governor "bw_hwmon"
    write /sys/class/devfreq/soc:qcom,cpubw/polling_interval 50
    write /sys/class/devfreq/soc:qcom,cpubw/min_freq 1525
    write /sys/class/devfreq/soc:qcom,cpubw/max_freq 13763
    write /sys/class/devfreq/soc:qcom,cpubw/bw_hwmon/mbps_zones "3143 5859 11863 13763"
    write /sys/class/devfreq/soc:qcom,cpubw/bw_hwmon/sample_ms 2
    write /sys/class/devfreq/soc:qcom,cpubw/bw_hwmon/io_percent 40
    write /sys/class/devfreq/soc:qcom,cpubw/bw_hwmon/hyst_length 10
    write /sys/class/devfreq/soc:qcom,cpubw/bw_hwmon/hist_memory 20
    write /sys/class/devfreq/soc:qcom,cpubw/bw_hwmon/guard_band_mbps 0
    write /sys/class/devfreq/soc:qcom,cpubw/bw_hwmon/up_scale 250
    write /sys/class/devfreq/soc:qcom,cpubw/bw_hwmon/idle_mbps 1600

    # Setup power retention
    write /sys/module/lpm_levels/system/pwr/cpu0/ret/idle_enabled N
    write /sys/module/lpm_levels/system/pwr/cpu1/ret/idle_enabled N

    write /sys/devices/system/cpu/cpu0/sched_load_boost 1
    write /sys/devices/system/cpu/cpu1/sched_load_boost 1
    write /sys/devices/system/cpu/cpu2/sched_load_boost 1
    write /sys/devices/system/cpu/cpu3/sched_load_boost 1

	  # Swappiness
	  write /proc/sys/vm/swappiness 20

    # Enable dynamic memory bus latency control
    write /sys/class/devfreq/soc:qcom,memlat-cpu0/governor "mem_latency"
    write /sys/class/devfreq/soc:qcom,memlat-cpu0/polling_interval 10
    write /sys/class/devfreq/soc:qcom,memlat-cpu0/mem_latency/ratio_ceil 400
    write /sys/class/devfreq/soc:qcom,memlat-cpu2/governor "mem_latency"
    write /sys/class/devfreq/soc:qcom,memlat-cpu2/polling_interval 10
    write /sys/class/devfreq/soc:qcom,memlat-cpu2/mem_latency/ratio_ceil 400

    write /sys/class/devfreq/soc:qcom,mincpubw/governor "cpufreq"

    # Enable UFS powersaving
    write /sys/module/lpm_levels/parameters/sleep_disabled N
